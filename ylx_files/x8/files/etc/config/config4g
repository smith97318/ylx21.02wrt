
config 4G '4G'
	option apn '3gnet'
	option enable '1'
	option auth '0'
	option tty '/dev/ttyUSB7'
	option tool 'at'
	option atcmd 'AT+QNETDEVCTL=1,1,1'
	option netdevice 'usb1'

